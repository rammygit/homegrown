    "fs-extra": "^8.1.0",
    "highlight.js": "^9.15.8",
    "marked": "^0.7.0",
    "pretty": "^2.0.0"
  },
  "devDependencies": {
    "bulma": "^0.7.5",
    "node-sass": "^4.12.0"
  }
}
