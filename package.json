{
  "name": "staticfier",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "css-build": "node-sass --omit-source-map-url sass/mystyles.scss css/mystyle.css",
    "css-watch": "npm run css-build -- --watch",
    "clean-css": "cleancss -o css/mystyle.min.css css/mystyle.css && cleancss -o css/main.min.css css/main.css && cleancss -o css/normalize.min.css css/normalize.css",
    "start": "npm install && npm run css-build && npm run clean-css && node server.js"
  },
